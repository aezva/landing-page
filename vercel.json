{
  "version": 2,
  "builds": [
    {
      "src": "frontend/client-platform/package.json",
      "use": "@vercel/next",
      "config": {
        "rootDirectory": "frontend/client-platform"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/client-platform/$1"
    }
  ]
} 